{
  "rewrites": [
    {
      "source": "/",
      "destination": "/api/index"
    }
  ],
  "functions": {
    "api/index.ts": {
      "maxDuration": 10,
      "includeFiles": "public/pages/verify.html"
    },
    "api/interactions.ts": {
      "maxDuration": 10,
      "includeFiles": "{src/{commands,components,utils,config}/**,node_modules/@google/generative-ai/**}"
    },
    "api/discord-oauth-callback.ts": {
      "maxDuration": 10,
      "includeFiles": "{dist/**,public/pages/**}"
    }
  },
  "buildCommand": "npm run build",
  "outputDirectory": ".",
  "installCommand": "npm install"
}
